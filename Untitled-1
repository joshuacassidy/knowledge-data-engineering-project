PREFIX	ex:	<http://www.example.org/ontologies/people#>
PREFIX re: <http://www.w3.org/2000/10/swap/reason#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?student ?hobby ?studentValue ?hobbyValue # ?studentValue ?hobbyValue ?student ?studentProperty ?hobby
WHERE { 
    ?student a ex:Student.
    ?student ?studentProperty ?studentValue.
	?hobby a ex:Hobby.
    ?hobby ?hobbyProperty ?hobbyValue.
    FILTER EXISTS {	?student ex:hasHobby ?hobby }
 	FILTER(	?studentProperty = ex:firstName && ?hobbyProperty = ex:name )
#	FILTER EXISTS {	?studentProperty = ?hobby }
#  	FILTER(	?hobbyValue = "Programming" && ?studentProperty = ex:firstName )
}